{
	"name": "Copy [Title](URL)",
	"version": "2.0",
	"manifest_version": 2,
	"description": "Lets you copy Title + URL",
	"icons": { 
		"16": "img/icon16.png",				
		"32": "img/icon32.png",		
		"48": "img/icon48.png",		
		"128": "img/icon128.png"	
	},
	"background": {
    	"page": "background.html"
    },
	"permissions": [ 
		"contextMenus",
		"unlimitedStorage",
		"tabs",
		"http://*/*",
		"https://*/*",
		"clipboardRead",
		"clipboardWrite"		
	]
}